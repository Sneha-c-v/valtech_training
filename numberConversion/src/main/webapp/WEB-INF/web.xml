<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app id="WebApp_ID">
<display-name>numberConversion</display-name>
  <servlet>
    <servlet-name>NumberSystemServlet</servlet-name>
    <servlet-class>numberConversion.NumberSystemServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NumberSystemServlet</servlet-name>
    <url-pattern>/NumberSystemServlet</url-pattern>
  </servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>convert.jsp</welcome-file>
	</welcome-file-list>
</web-app>